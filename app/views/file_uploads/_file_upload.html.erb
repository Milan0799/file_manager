<div id="<%= dom_id file_upload %>">
  <p>
    <strong>Title:</strong>
    <%= file_upload.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= file_upload.description %>
  </p>

  <p>
    <strong>File type:</strong>
    <%= file_upload.file_type %>
  </p>

  <p>
    <strong>Public token:</strong>
    <%= file_upload.public_token %>
  </p>

  <p>
    <strong>User:</strong>
    <%= file_upload.user_id %>
  </p>

</div>
