# Ignore bundler files
.bundle

# Ignore log files
/log/*
!/log/.keep

# Ignore tmp files
/tmp/*
!/tmp/.keep

# Ignore system-specific files
*.swp
*.swo
.DS_Store

# Ignore Rails secrets and keys
config/master.key
config/credentials.yml.enc

# Ignore assets and cache
/public/assets
/public/packs
/node_modules
.yarn-error.log
.byebug_history

# Ignore database files
*.sqlite3
*.sqlite3-journal

# Ignore pid and restart files
/tmp/pids/*
!/tmp/pids/.keep

# Ignore coverage files (generated by test coverage tools like SimpleCov)
coverage/
